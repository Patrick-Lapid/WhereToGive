{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "http://localhost:8080",
    "moduleResolution": "node",
    "lib": [ "es2015", "dom"],
    "types": ["node", "cypress", "cypress-react-selector"]
    },
  "include": [
    "src",
    "decs.d.ts",
    "cypress/support/**/*.ts"
  ]
}
